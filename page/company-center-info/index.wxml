        <view>
          <text class='person-name'>发票信息</text>
        </view>
        <view>
          <text class='auth-good-descr'>消费开发票</text>
        </view>
      </view>
      <view class='float-right'> 
        <image src='../../image/company-center/row-right.png' class='go-icon'></image>
      </view>
    </view>
  </view>
  <view class='program-list'>
    <view class='program-item'>
      <view class='col-20 img-center'>
        <image src='../../image/post/contacts.png' class='program-img'></image>
      </view>
      <view class='col-60'>
        <view class='program-title'>新大课堂</view>
        <view class='program-describe'>副标题描述</view>
      </view>
      <view class='col-20'>
        <view class='concer-btn'>关注</view>
      </view>
    </view>
  </view>
  <view class='change-city'>切换城市</view>
  <view class='change-city'>退出账号</view>
</view>