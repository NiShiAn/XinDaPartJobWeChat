.swipter-containt {
  width: 100%;
  height: 100%;
  overflow: hidden;
  overflow-y: scroll;
  background-color: #f2f3f8;
}

.swipter-item {
  background-color: #fff;
  padding: 20rpx 20rpx 10rpx 20rpx;
  margin-bottom: 40rpx;
}

.row {
  overflow: hidden;
  width: 100%;
}

.col-75 {
  width: 75%;
  float: left;
  overflow: hidden;
}

.col-25 {
  width: 25%;
  float: left;
  overflow: hidden;
  text-align: center;
}

.delete-img {
  width: 30rpx;
  height: 40rpx;
}

.edite-img {
  width: 30rpx;
  height: 40rpx;
}

.btn-row {
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.line-shu {
  width: 3rpx;
  height: 30rpx;
  background-color: #ddd;
  margin-left: 20rpx;
  margin-right: 20rpx;
  position: relative;
  top: 10rpx;
}

.party-work-name {
  margin-bottom: 20rpx;
  font-size: 38rpx;
  color: #333;
}

.work-describe{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  color: #666;
  font-size: 32rpx;
  margin-bottom: 30rpx;
}

.is-release {
  margin-left: 20rpx;
  color: orange;
}

.release-time {
  font-size: 32rpx;
  color: #666;
  margin-bottom: 10rpx;
}

.go-up {
  display: inline-block;
  margin-top: 20rpx;
  padding: 10rpx 30rpx;
  border: 1px solid #ddd;
  color: #333;
  font-size: 38rpx;
  border-radius: 10rpx;
}