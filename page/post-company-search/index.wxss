/**index.wxss**/
.container {
  background-color: #fff;
  min-height: 100%;
  overflow-x: hidden;
}
.search-container{
  height: 74rpx;
  width: 100%;
  background: #efeff4;
  padding-top:14rpx;
  position: relative;
}
.search-img{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top:28rpx;
  left: 32rpx;
}
.search-input{
  width: 74%;
  padding-left: 8%;
  height: 60rpx;
  background: white;
  margin-left: 3%;
  border-radius: 8rpx;
  font-size: 28rpx;
  color: black;
}
.cancle{
  font-size: 32rpx;
  color: rgb(0, 194, 3);
  position: absolute;
  top:24rpx;
  right: 32rpx;
}
.title-line{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 5%;
  padding-right: 5%;
  clear: both;
}
.title-line span{
  display: block;
  width: 50%;
  height: 80rpx;
  line-height: 80rpx;
  float: left;
   font-size: 30rpx;
  color: rgb(53, 53, 53);
}

.title-line image{
  display: block;
  width: 30rpx;
  height:30rpx;
  float: right;
  margin-top: 24rpx;
}
.em-list{
  width: 92%;
  min-height: 200rpx;
  padding-left: 4%;
  padding-right: 4%;
  padding-top: 20rpx;
}
.em-item{
  display: block;
  width: 148rpx;
  height: 58rpx;
  line-height: 58rpx;
  border:2rpx solid rgb(217, 217, 217);
  border-radius: 8rpx;
  font-size: 26rpx;
  color: rgb(136, 136, 136);
  float: left;
  text-align: center;
  margin-left: 10rpx;
  margin-right: 10rpx;
  margin-bottom: 20rpx;
}
.em-item-select{
  border:2rpx solid rgb(230, 67, 64);
 color: rgb(230, 67, 64);
}